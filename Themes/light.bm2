PopMenu[
    QMenu {
        background-color: #1886D1;
        border-left: 2px solid #2CA4E0;
        border-radius:0px;
        color:white;
        border-top:0px solid red;
    }
    QMenu::separator{
        background-color:#2CA4E0;
        height:1px;
    }
    QMenu::item {
        padding:10px;
        padding-right:70px;
        padding-left:15px;
        width:120px;
    }
    QMenu::item::disabled{
        background-color:#6fc1ea;
    }
    QMenu::item::disabled:selected{
        background-color:#6fc1ea;
    }
    QMenu::item:selected {
        background-color: #10588A;
    }
    QMenu::item:pressed {
        background-color: #11598B;
    }
]
PopMenu2[
