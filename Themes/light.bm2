        border-top:0px solid red;
    }
    QMenu::item {
        padding:10px;
        padding-right:70px;
        padding-left:15px;
    }
    QMenu::item::disabled{
        background-color:white;
        color:lightgrey;
    }
    QMenu::item::disabled:selected{
