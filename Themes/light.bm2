        }
]

titleBar[
    .QPushButton{
        background-color:#2CA4E0;
        border:0;
        text-align:left;
    }
]

scrollBar[
    QScrollBar:vertical {               
            border: 0px solid #66b1e4;
            background:#d9e9f4;
            background-color:#d9e9f4;
            width:10px;
            height:20px;
            border-right:2px solid #2CA4E0;
        }
    QScrollBar:vertical::pressed {               
            background:white;
            background-color:white;
        }
        QScrollBar::handle:vertical {
            border-radius:0px;
            background: #3498db;
            background-color: #2CA4E0;
            min-height: 100px;
        }
        QScrollBar::handle:vertical::pressed {
            background-color: #45a0de;

        }
        QScrollBar::add-line:vertical {
            height: 20px;
            border-right:2px solid #2CA4E0;
            subcontrol-position: bottom;
            subcontrol-origin: margin;
            background:white;
        }
        QScrollBar::sub-line:vertical {
            border-right:2px solid #2CA4E0;
            height: 20px;
            background:white;
            subcontrol-position: bottom;
            subcontrol-origin: margin;
        }
    QScrollBar::handle:disabled{ 
        background-color:#a5dbff;
        border:0px;
    }
]

