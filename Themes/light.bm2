#name Light Theme
MainBody[
    border:0px solid grey;
    background-color:#EAEDF2;
    font-family:'Ubuntu Light';
]
closeBtn[
    .QPushButton{
        background-color:#2CA4E0;
        border:0;
        padding-left:15px;
        padding-right:15px;
        } 
    .QPushButton:hover{
        background-color:#1f97d3;
        }
]
